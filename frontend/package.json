{
    "name": "db-web-ui",
    "version": "0.0.0",
    "scripts": {
        "clean": "rm -rf node_modules",
        "ng": "ng",
        "start": "ng run frontend:serve:local --ssl true",
        "start-mock": "ng run frontend:serve-mock:local",
        "build": "ng build --configuration=production",
        "build-dev": "ng build --configuration=local",
        "test": "ng test --configuration=local --watch=false --code-coverage",
        "test-remote": "ng test --configuration=remote --watch=false",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "ng lint",
        "prepare": "cd .. && husky install frontend/.husky",
        "lint-staged": "lint-staged",
        "cypress:open": "cypress open",
        "cypress:chrome": "ng run frontend:cypress --browser chrome",
        "cypress:firefox": "ng run frontend:cypress --browser firefox"
    },
    "private": true,
    "devDependencies": {
        "@angular-builders/custom-webpack": "14.1.0",
        "@angular-devkit/build-angular": "14.2.10",
        "@angular-eslint/builder": "14.4.0",
        "@angular-eslint/eslint-plugin": "14.4.0",
        "@angular-eslint/eslint-plugin-template": "14.4.0",
        "@angular-eslint/schematics": "14.4.0",
        "@angular-eslint/template-parser": "14.4.0",
        "@angular/cli": "14.2.10",
        "@angular/compiler-cli": "14.2.12",
        "@angular/language-service": "14.2.12",
        "@babel/core": "7.17.8",
        "@babel/preset-env": "7.16.11",
        "@babel/runtime": "7.17.8",
        "@types/jasmine": "4.0.3",
        "@types/jasminewd2": "2.0.10",
        "@types/lodash": "4.14.182",
        "@types/node": "16.11.49",
        "@types/sanitize-html": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "5.19.0",
        "@typescript-eslint/parser": "5.19.0",
        "babel-loader": "8.2.4",
        "connect-prism": "1.0.21",
        "cypress": "12.3.0",
        "diff-package-lock": "0.1.31",
        "eslint": "8.13.0",
        "eslint-config-prettier": "8.5.0",
        "husky": "8.0.1",
        "jasmine-core": "4.0.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.3.19",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "4.0.1",
        "karma-jasmine-html-reporter": "1.7.0",
        "karma-selenium-launcher": "1.0.1",
        "karma-spec-reporter": "0.0.34",
        "lint-staged": "12.4.1",
        "prettier": "2.6.2",
        "prettier-plugin-organize-imports": "2.3.4",
        "ts-node": "10.7.0",
        "typescript": "4.6.3"
    },
    "engines": {
        "node": "16.17.0"
    },
    "dependencies": {
        "@angular/animations": "14.2.12",
        "@angular/cdk": "14.2.7",
        "@angular/common": "14.2.12",
        "@angular/compiler": "14.2.12",
        "@angular/core": "14.2.12",
        "@angular/forms": "14.2.12",
        "@angular/localize": "14.2.12",
        "@angular/material": "14.2.7",
        "@angular/platform-browser": "14.2.12",
        "@angular/platform-browser-dynamic": "14.2.12",
        "@angular/router": "14.2.12",
        "@cypress/schematic": "2.4.0",
        "@ng-bootstrap/ng-bootstrap": "13.1.1",
        "@ng-select/ng-select": "9.1.0",
        "@ngx-loading-bar/core": "6.0.2",
        "@ngx-loading-bar/http-client": "6.0.2",
        "@technical-design/ripe-app-webcomponents": "3.3.0",
        "@types/crypto-js": "4.1.1",
        "angulartics": "1.6.0",
        "angulartics-piwik": "1.0.6",
        "bootstrap": "5.1.3",
        "classlist": "2.0.0",
        "cookie-parser": "1.4.6",
        "crypto-js": "4.1.1",
        "ip-address": "8.1.0",
        "lodash": "4.17.21",
        "moment": "2.29.4",
        "ng-diff-match-patch": "3.0.1",
        "ngx-cookie-service": "14.0.1",
        "npm": "8.19.2",
        "rxjs": "7.5.5",
        "sanitize-html": "^2.7.0",
        "tslib": "2.3.1",
        "whatwg-fetch": "3.6.2",
        "zone.js": "0.11.8"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    }
}
