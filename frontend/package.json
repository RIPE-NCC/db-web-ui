{
  "name": "db-web-ui",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf node_modules",
    "ng": "ng",
    "start": "ng run frontend:serve:local --ssl true",
    "start-mock": "ng run frontend:serve-mock:local",
    "build": "ng build --configuration=production",
    "build-dev": "ng build --configuration=local",
    "test": "ng test --configuration=local --watch=false --code-coverage",
    "test-remote": "ng test --configuration=remote --watch=false",
    "lint": "ng lint",
    "pree2e-chrome": "webdriver-manager update --standalone false --versions.chrome 99.0.4844.51",
    "e2e-chrome": "ng e2e --no-webdriver-update --configuration=local",
    "pree2e-firefox": "webdriver-manager update --standalone false",
    "e2e-firefox": "ng e2e --no-webdriver-update --configuration=firefox",
    "e2e-remote": "ng e2e --configuration=remote"
  },
  "private": true,
