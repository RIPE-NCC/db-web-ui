<!-- Usersnap widget: always on -->
<script>
    window.onUsersnapLoad = function(api) {
        window.Usersnap = api;
        api.init({
            fields: {
                comment: {
                    defaultValue: '',
                    label: 'Your message',
                    required: true,
                    multiline: true
                },
                email: {
                    defaultValue: RIPE.usermail,
                    label: 'Your E-mail',
                    required: true
                }
            },
            button: {
              icon: null,
              label: 'Report a Bug',
              position: 'bottomRight'
            }
        });

        api.on("open", function(event) {
            event.api.setValue('customData', "db-web-ui-" + angular.element(document).injector().get('Properties').BUILD_TAG);
        });
    }
</script>
<script src="//api.usersnap.com/load/b175ab64-1ecd-40a6-b5e3-ff8a1516fe6f.js?onload=onUsersnapLoad" async ></script>
